{
  "name": "Menu-todo",
  "title": "Menu Todo",
  "version": "1.0.0",
  "description": "Todo on macOS menu bar",
  "main": "app/main.js",
  "scripts": {
    "start": "webpack serve --config webpack.config.js & electron .",
    "build": "tsc",
    "deploy": "webpack --config webpack.config.js --mode production && electron-builder build --publish never -c electron-builder.env",
    "test": "npx jest"
  },
  "postinstall": "electron-builder install-app-deps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tulburg/menu-todo.git"
  },
  "keywords": [
    "menu",
    "bar",
    "todo"
  ],
  "author": "jsnative",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tulburg/menu-todo/issues"
  },
  "homepage": "https://github.com/tulburg/menu-todo#readme",
  "dependencies": {
    "@javascriptui/core": "^0.0.6",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "electron": "^21.2.0",
    "electron-store": "^8.1.0",
    "electron-util": "^0.17.2",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "devDependencies": {
    "electron-builder": "^23.6.0"
  }
}
